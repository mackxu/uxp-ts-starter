{
  "id": "com.upx.starter",
  "name": "Upx Starter",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 6,
  "host": [
    {
      "app": "PS",
      "minVersion": "24.4.0"
    }
  ],
  "requiredPermissions": {
    "webview": {
      "allow": "yes",
      "domains": "all",
      "enableMessageBridge": "localAndRemote"
    },
    "network": {
      "domains": "all"
    }
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "starter",
      "label": "uxp starter",
      "minimumSize": {
        "width": 320,
        "height": 850
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 750,
        "height": 720
      },
      "preferredFloatingSize": {
        "width": 375,
        "height": 720
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "images/icon@1x.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "images/icon@2x.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 32,
      "height": 32,
      "path": "images/icon@1x.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "dark",
        "darkest"
      ],
      "species": [
        "generic"
      ]
    },
    {
      "width": 32,
      "height": 32,
      "path": "images/icon@2x.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ],
      "species": [
        "generic"
      ]
    }
  ]
}